  "bugs": {
    "url": "https://github.com/lucagem/node-red-contrib-mqtt-gem/issues"
  },
  "homepage": "https://github.com/lucagem/node-red-contrib-mqtt-gem#readme",
  "node-red": {
    "nodes": {
      "mqtt-gem": "mqtt-gem.js"
    }
  },
  "dependencies": {
